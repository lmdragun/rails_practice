
<img src="<%= @entry.photo_url %>">

<h2>by <%= @entry.author %></h2>
<h3>Created on: <%= @entry.created_at.to_date %></h3>
<a href="/entries/<%=@entry.id%>/edit">Update this entry</a>

<div class="button">
<%= form_tag entry_path(@entry), method: "delete" do %>
	<%= submit_tag("destroy") %>
<% end %>
</div>

<a href="/entries" class="home">Return Home</a>
